{
    "manifest_version": 2,
    "name": "Alles Search",
    "short_name": "alles-search",
    "description": "Adds search.alles.cx as a search engine.",
    "version": "1.0.2",
    "icons": {
		"192": "icons/192x192.png"
	},
    "chrome_settings_overrides": {
        "search_provider": {
            "favicon_url": "https://search.alles.cx/_/static/logo-16.png",
            "is_default": true,
            "keyword": "alles",
            "encoding": "UTF-8",
            "name": "Alles",
            "search_url": "https://search.alles.cx/?q={searchTerms}"
        },
        "homepage": "https://search.alles.cx/"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
}
